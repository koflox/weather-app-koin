<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GraphicView">
        <attr name="colorGraphic" format="color" />
        <attr name="colorData" format="color" />
        <attr name="dataType" format="enum">
            <enum name="none" value="0" />
            <enum name="hourly_temp" value="1" />
            <enum name="precipitation" value="2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="HeaderTextView">
        <attr name="strokeColor" format="color" />
        <attr name="strokePaddingStart" format="dimension" />
        <attr name="strokePaddingEnd" format="dimension" />
        <attr name="strokeWidth" format="dimension" />
    </declare-styleable>
</resources>